{
  "name": "react_starter",
  "version": "1.0.0",
  "description": "React starter",
  "scripts": {
    "postinstall": "yarn run build:dll",
    "start": "node server.js",
    "start:dashboard": "webpack-dashboard -- node server.js",
    "start:analyze": "node server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "yarn run clean:build && webpack --config webpack.build.js",
    "build:analyze": "yarn run clean:build && webpack --config webpack.build.js",
    "build:dll": "yarn run clean:dll && webpack --config webpack.dll.js",
    "clean:dll": "rimraf -- src/dll",
    "clean:build": "rimraf -- build",
    "prettier": "prettier-eslint --write \"**/*.js\""
  },
  "main": "index.js",
  "repository": {
    "url": "https://github.com/plugged/react-starter.git",
    "type": "git"
  },
  "author": "Vil",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^7.0.0-alpha.15",
    "history": "^4.6.3",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-beta.5",
    "react-dom": "^16.0.0-beta.5",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^7.0.0-alpha.15",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^7.0.0-alpha.15",
    "babel-preset-react": "^7.0.0-alpha.15",
    "babel-preset-stage-0": "^7.0.0-alpha.15",
    "cache-loader": "^1.0.3",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "express": "^5.0.0-alpha.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "happypack": "^4.0.0-beta.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "prettier-eslint-cli": "^4.2.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dashboard": "^1.0.0-3",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  }
}
