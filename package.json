{
  "name": "react_starter",
  "version": "1.0.0",
  "description": "React starter",
  "scripts": {
    "postinstall": "yarn run build:dll",
    "start": "node server.js",
    "start:analyze": "node server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "yarn run clean:build && webpack --config webpack.build.js",
    "build:analyze": "yarn run clean:build && webpack --config webpack.build.js",
    "build:dll": "yarn run clean:dll && webpack --config webpack.dll.js",
    "clean:dll": "rimraf -- src/dll",
    "clean:build": "rimraf -- build"
  },
  "main": "index.js",
  "repository": {
    "url": "https://github.com/plugged/react-starter.git",
    "type": "git"
  },
  "author": "Vil",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "history": "^4.6.1",
    "prop-types": "^15.5.7",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.0",
    "react-router-redux": "5.0.0-alpha.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "happypack": "^3.0.3",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  }
}
