{
  "name": "react_starter",
  "version": "1.0.0",
  "description": "React starter",
  "scripts": {
    "postinstall": "yarn run build:dll",
    "start": "node server.js",
    "start:analyze": "node server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "yarn run clean:build && webpack --config webpack.build.js",
    "build:analyze": "yarn run clean:build && webpack --config webpack.build.js",
    "build:dll": "yarn run clean:dll && webpack --config webpack.dll.js",
    "clean:dll": "rimraf -- src/dll",
    "clean:build": "rimraf -- build"
  },
  "main": "index.js",
  "repository": {
    "url": "https://github.com/plugged/react-starter.git",
    "type": "git"
  },
  "author": "Vil",
  "license": "MIT",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "happypack": "^3.0.2",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.2.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  }
}
