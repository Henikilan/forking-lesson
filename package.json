{
  "name": "react_starter",
  "version": "1.0.0",
  "description": "React starter",
  "scripts": {
    "postinstall": "yarn run build:dll",
    "start": "node server.js",
    "start:dashboard": "webpack-dashboard -- node server.js",
    "start:analyze": "node server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "yarn run clean:build && webpack --config webpack.build.js",
    "build:analyze": "yarn run clean:build && webpack --config webpack.build.js",
    "build:dll": "yarn run clean:dll && webpack --config webpack.dll.js",
    "clean:dll": "rimraf -- src/dll",
    "clean:build": "rimraf -- build",
    "prettier": "prettier-eslint --write \"**/*.js\""
  },
  "main": "index.js",
  "repository": {
    "url": "https://github.com/plugged/react-starter.git",
    "type": "git"
  },
  "author": "Vil",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^7.0.0-alpha.12",
    "history": "^4.6.1",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-alpha.12",
    "react-dom": "^16.0.0-alpha.12",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^7.0.0-alpha.12",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^7.0.0-alpha.12",
    "babel-preset-react": "^7.0.0-alpha.12",
    "babel-preset-stage-0": "^7.0.0-alpha.12",
    "cache-loader": "^1.0.3",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "express": "^5.0.0-alpha.5",
    "extract-text-webpack-plugin": "^2.1.0",
    "happypack": "^4.0.0-beta.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "prettier-eslint-cli": "^4.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
